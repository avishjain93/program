
public class MultipleOf 
{
	public static void main(String[] args) 
	{
		for(int i=1;i<=50;i++)
		{
			System.out.print(i);
			if(i%3==0 && i%5==0)
			{
				System.out.print(" Multiple of 3 & 5");
			}
			else
			if(i%3==0)
			{
				System.out.print(" Multiple of 3");
			}
			else
			if(i%5==0)
			{
				System.out.print(" Multiple of 5");
			}
			
		}

	}

}
